<h1> Welcome to Members Only! </h1>

<%= link_to "See all Posts", posts_path %> <br>
<% if logged_in? %>
    <%= link_to "Go to dashboard", user_path(session[:user_id]) %>
    <%= link_to "Logout", '/logout', method: :delete, data: {confirm: "Are you sure you want to log out?"}%>
<% else %>
    <%= link_to "Sign Up", new_user_path %>
    <%= link_to "Login",  '/login' %>
<% end %>